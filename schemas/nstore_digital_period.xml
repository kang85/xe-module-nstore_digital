<table name="nstore_digital_period">
	<column name="period_srl" type="number" size="11" notnull="notnull" index="idx_period_srl" />
	<column name="cart_srl" type="number" size="11" notnull="notnull" index="idx_cart_srl" />
	<column name="order_srl" type="number" size="11" default="0" notnull="notnull" index="idx_order_srl" />
	<column name="member_srl" type="number" size="11" notnull="notnull" index="idx_member_srl" />
	<column name="module_srl" type="number" size="11" default="0" notnull="notnull" index="idx_module_srl" />
	<column name="item_srl" type="number" size="11" notnull="notnull" index="idx_item_srl" />
	<column name="price" type="number" size="11" default="0" notnull="notnull" />
	<!-- 0: 카트보관, 1: 입금대기, 2:입금완료, 3: 구매완료, A: 최소, B: 반품,교환, C: 환불 -->
	<column name="order_status" type="char" size="1" default="0" notnull="notnull" index="idx_order_status" />
	<column name="start_date" type="number" size="11" notnull="notnull" index="idx_start_date" />
	<column name="end_date" type="number" size="11" notnull="notnull" index="idx_end_date" />
	<column name="payment_method" type="varchar" size="2" notnull="notnull" />
	<column name="regdate" type="date" notnull="notnull" />
</table>
